<?xml version="1.0"?>
<launch>

    <node pkg="interestingness_ros" type="interestingness_node.py" name="interestingness_node" output="screen">
        <remap from="/rs_front/color/image" to="/rs_front/color/image"/>
        <param name="image-topic" value="/rs_front/color/image" />
        <param name="data-root" value="/data/datasets" />
        <param name="model-save" value="$(find interestingness_ros)/saves/ae.pt.SubTF.interest.mse" />
        <param name="crop-size" value="320" />
        <param name="num-interest" value="10" />
        <param name="skip-frames" value="5" />
        <param name="window-size" value="1" />
        <param name="save-flag" value="test" />
        <param name="rr" value="5" />
        <param name="wr" value="5" />
    </node>

    <node pkg="interestingness_ros" type="interest_marker.py" name="interestmarker_node" output="screen">
        <remap from="/interestingness/info" to="/interestingness/info"/>
        <param name="min-level" value="0.1" />
    </node>

</launch>